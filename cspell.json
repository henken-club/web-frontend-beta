{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "files": [
    "src/**/*",
    "schema.graphql"
  ],
  "ignorePaths": [
    "*.snap",
    "*.codegen.*",
    "codegen.ts",
    "src/i18n/i18n-*"
  ],
  "words": [
    "bookcover",
    "bookcovers",
    "bookseries",
    "codegen",
    "followee",
    "followees",
    "henken",
    "henkens",
    "reexecute",
    "typesafe",
    "unfollow",
    "viewports",
    "xmark"
  ],
  "ignoreWords": [
    "fontawesome",
    "fortawesome",
    "urql",
    "nextjs",
    "clsx"
  ]
}

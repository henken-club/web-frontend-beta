    "eslint-config-next": "11.1.2",
    "eslint-plugin-unused-imports": "1.1.4",
    "faker": "5.5.3",
    "jest": "27.2.1",
    "msw": "0.35.0",
    "msw-storybook-addon": "1.3.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.7",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-next-router": "3.0.8",
    "tailwindcss": "2.2.15",
    "ts-jest": "27.0.5",
